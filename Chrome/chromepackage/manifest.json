{
    "name": "ibrowser",
    "version": "0.1.2",
    "manifest_version": 2,
    "update_url":"http://liang8305.github.com/ibrowser/updates.html",
    
    "permissions": [
        "notifications",
        "alarms",
        "tabs",
        "http://*/*"
    ],
    "plugins": [
        {
                "path": "ibrowser_x86_64.plugin","public":true
        } 
    ],
    "browser_action": {
        "default_title": "ibrowser",
        "default_icon": "icon.jpeg",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "background.html",
        "persistent": true
    }
    
}
